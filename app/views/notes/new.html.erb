<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@note) do |f| %>
          <%= render 'shared/error_messages' %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :content %>
      <%= f.text_field :content %>

      <%= f.submit "Добавить", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
